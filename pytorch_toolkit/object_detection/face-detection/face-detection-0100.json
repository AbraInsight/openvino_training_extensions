                256,
                256
            ],
            "keep_ratio": false
        },
        {
            "type": "Normalize",
            "mean": [
                0,
                0,
                0
            ],
            "std": [
                255,
                255,
                255
            ],
            "to_rgb": true
        },
        {
            "type": "RandomFlip",
            "flip_ratio": 0.5
        },
        {
            "type": "DefaultFormatBundle"
        },
        {
            "type": "Collect",
            "keys": [
                "img",
                "gt_bboxes",
                "gt_labels"
            ]
        }
    ],
    "test_pipeline": [
